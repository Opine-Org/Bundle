{
    "name": "opine/bundle",
    "type": "library",
    "version": "2.0.0",
    "description": "Embed Opine projects withing other Opine projects",
    "keywords": [
        "bundle", "plugin"
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name"    : "Ryan Mahoney",
            "email"   : "ryan@virtuecenter.com",
            "homepage": "http://opine-php.org",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php"             : ">=5.5.0",
        "symfony/yaml"    : "2.6.1",
        "opine/container" : "2.0.3",
        "opine/cache"     : "2.0.3",
        "opine/route"     : "3.0.2",
        "opine/config"    : "3.0.0",
        "opine/interfaces": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.32",
        "opine/semantic-cm": "1.5.7"
    },
    "autoload": {
        "psr-4": {
            "Opine\\Bundle\\": "src/"
        }
    }
}